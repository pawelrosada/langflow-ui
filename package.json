{
  "name": "langflow-chat-ui",
  "version": "1.0.0",
  "description": "Modern ChatGPT-like interface for Langflow integration",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:backend\" \"pnpm dev:frontend\"",
    "dev:backend": "tsx watch src/backend/index.ts",
    "dev:frontend": "vite --host",
    "build": "pnpm build:backend && pnpm build:frontend",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "vite build",
    "start": "node dist/backend/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "docker:dev": "./scripts/dev.sh",
    "docker:prod": "./scripts/prod.sh",
    "docker:build": "docker build -t langflow-chat-ui .",
    "docker:run": "docker run -p 3000:3000 --env NODE_ENV=production langflow-chat-ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.1",
    "@fastify/websocket": "^10.0.1",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "fastify": "^4.26.2",
    "ioredis": "^5.3.2",
    "pino-pretty": "^13.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "framer-motion": "^11.0.6",
    "lucide-react": "^0.338.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.15.0"
  },
  "packageManager": "pnpm@8.15.0"
}
